{
	"compilerOptions": {
		"target": "ES2018",
		"module": "ESNext",
		"moduleResolution": "Node",
		"lib": ["ESNext", "ESNext.AsyncIterable", "DOM"],
		"esModuleInterop": true,
		"allowJs": true,
		"sourceMap": true,
		"strict": true,
		"noEmit": true,
		"experimentalDecorators": true,
		"baseUrl": ".",
		"paths": {
			"@assets*": ["./assets*"],
			"@components*": ["./components*"],
			"@layouts*": ["./layouts*"],
			"@middleware*": ["./middleware*"],
			"@pages*": ["./pages*"],
			"@plugins*": ["./plugins*"],
			"@static*": ["./static*"],
			"@store*": ["./store*", "./store/index.ts"],
			"@includes*": ["./includes*", "includes/index.ts"],
			"~/*": ["./*"],
			"@/*": ["./*"]
		},
		"types": [
			"@nuxt/types",
			"@nuxtjs/axios",
			"@types/node",
			"@nuxtjs/moment",
			"moment-timezone",
			"@types/workbox-sw"
		],
		"typeRoots": ["./includes"]
	},
	"exclude": ["node_modules", ".nuxt", "dist"]
}
